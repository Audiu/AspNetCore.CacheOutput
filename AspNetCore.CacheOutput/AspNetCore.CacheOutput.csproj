<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netstandard2.0</TargetFrameworks>
    <AssemblyName>AspNetCore.CacheOutput</AssemblyName>
    <Company />
    <Authors>Alexander Shabunevich</Authors>
    <Description>ASP.NET Core port of Strathweb.CacheOutput library (https://github.com/filipw/Strathweb.CacheOutput)</Description>
    <PackageTags>aspnet, core, netcore, cacheoutput, cache</PackageTags>
	<Version>1.0.10</Version>
	<PackageLicenseUrl>https://github.com/Iamcerba/AspNetCore.CacheOutput/blob/master/LICENSE</PackageLicenseUrl>
	<PackageProjectUrl>https://github.com/Iamcerba/AspNetCore.CacheOutput</PackageProjectUrl>
	<RepositoryUrl>https://github.com/Iamcerba/AspNetCore.CacheOutput</RepositoryUrl>
	<RepositoryType>GitHub</RepositoryType>
	<Copyright>Copyright Â© 2018 Alexander Shabunevich</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.0.4" Condition="'$(TargetFramework)' == 'netcoreapp2.0'" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.1.3" Condition="'$(TargetFramework)' == 'netcoreapp2.1'" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.0" Condition="'$(TargetFramework)' == 'netcoreapp2.2' Or '$(TargetFramework)' == 'netstandard2.0'" />
  </ItemGroup>  

</Project>
